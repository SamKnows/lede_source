## Unit
CONFIG_TARGET_mvebu64=y
CONFIG_TARGET_mvebu64_armada-a8k-MACHIATOBin=y
CONFIG_DEVEL=y
CONFIG_EXTERNAL_KERNEL_TREE="/opt/samknows/openwrt-kernel"
CONFIG_TARGET_ROOTFS_EXT4FS=y

## Build
CONFIG_SDK=y

## C++
CONFIG_PACKAGE_libstdcpp=y
CONFIG_USE_LIBSTDCXX=y

## Busybox
CONFIG_BUSYBOX_CUSTOM=y
# Global
CONFIG_BUSYBOX_CONFIG_MONOTONIC_SYSCALL=y
# Shell
CONFIG_BUSYBOX_CONFIG_ASH_RANDOM_SUPPORT=y
# Utils
CONFIG_BUSYBOX_CONFIG_NC_SERVER=y
CONFIG_BUSYBOX_CONFIG_NC_EXTRA=y
CONFIG_BUSYBOX_CONFIG_PMAP=y
CONFIG_BUSYBOX_CONFIG_PSTREE=y
CONFIG_BUSYBOX_CONFIG_SHA1SUM=y
CONFIG_BUSYBOX_CONFIG_SHA512SUM=y
CONFIG_BUSYBOX_CONFIG_TASKSET=y
CONFIG_BUSYBOX_CONFIG_FEATURE_TASKSET_FANCY=y
CONFIG_BUSYBOX_CONFIG_TUNCTL=y

## Libraries
# System
CONFIG_PACKAGE_libpthread=y
CONFIG_PACKAGE_librt=y
CONFIG_PACKAGE_libminiupnpc=y

## Packages
CONFIG_PACKAGE_ca-certificates=y
CONFIG_PACKAGE_coreutils=y
CONFIG_PACKAGE_coreutils-timeout=y
CONFIG_PACKAGE_curl=y
CONFIG_LIBCURL_OPENSSL=y
CONFIG_LIBCURL_CRYPTO_AUTH=y
CONFIG_LIBCURL_PROXY=y
CONFIG_LIBCURL_TLS_SRP=y
CONFIG_LIBCURL_LIBIDN=y
CONFIG_LIBCURL_THREADED_RESOLVER=y
CONFIG_LIBCURL_ZLIB=y
CONFIG_PACKAGE_ethtool=y
CONFIG_PACKAGE_flock=y
CONFIG_PACKAGE_ip-full=y
CONFIG_PACKAGE_iperf3=y
CONFIG_PACKAGE_jq=y
CONFIG_PACKAGE_libatomic=y
CONFIG_PACKAGE_libnl=y
CONFIG_PACKAGE_openssh-sftp-server=y
CONFIG_PACKAGE_openvpn-openssl=y
CONFIG_OPENVPN_openssl_ENABLE_IPROUTE2=y
CONFIG_PACKAGE_procps-ng=y
CONFIG_PACKAGE_procps-ng-free=n
CONFIG_PACKAGE_procps-ng-kill=n
CONFIG_PACKAGE_procps-ng-pgrep=n
CONFIG_PACKAGE_procps-ng-pkill=n
CONFIG_PACKAGE_procps-ng-pmap=n
CONFIG_PACKAGE_procps-ng-ps=n
CONFIG_PACKAGE_procps-ng-pwdx=n
CONFIG_PACKAGE_procps-ng-skill=n
CONFIG_PACKAGE_procps-ng-slabtop=n
CONFIG_PACKAGE_procps-ng-snice=n
CONFIG_PACKAGE_procps-ng-tload=n
CONFIG_PACKAGE_procps-ng-top=y
CONFIG_PACKAGE_procps-ng-uptime=n
CONFIG_PACKAGE_procps-ng-vmstat=n
CONFIG_PACKAGE_procps-ng-w=n
CONFIG_PACKAGE_procps-ng-watch=n
CONFIG_PACKAGE_psmisc=y
CONFIG_PACKAGE_ss=y
CONFIG_PACKAGE_strace=y
CONFIG_PACKAGE_tcpdump-mini=y
CONFIG_PACKAGE_traceroute=y

## Kernel
CONFIG_KERNEL_FTRACE=y
CONFIG_KERNEL_ENABLE_DEFAULT_TRACERS=y
CONFIG_KERNEL_PROC_PAGE_MONITOR=y
CONFIG_KERNEL_PROFILING=y
CONFIG_PACKAGE_kmod-drop_monitor=y

## Disable
CONFIG_BUILD_PATENTED=n
CONFIG_BUSYBOX_CONFIG_FEATURE_NTPD_SERVER=n
CONFIG_BUSYBOX_CONFIG_FLOCK=n
CONFIG_BUSYBOX_CONFIG_KILLALL=n
CONFIG_BUSYBOX_CONFIG_TOP=n
CONFIG_BUSYBOX_CONFIG_TRACEROUTE=n

## SamKnows
CONFIG_PACKAGE_samknows_base-files_macchiato=y
